    "gh-pages": "^6.3.0",
    "history": "5.3.0",
    "i18next": "20.1.0",
    "i18next-browser-languagedetector": "6.1.0",
    "i18next-xhr-backend": "3.2.2",
    "react": "17.0.2",
    "react-bootstrap": "1.5.2",
    "react-chrono": "1.9.2",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-i18next": "11.8.12",
    "react-image-lightbox": "5.1.1",
    "react-lazyload": "3.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
